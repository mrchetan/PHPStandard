{
    "name": "mrchetan/php_standard",
    "description": "PHP coding standard to be followed in Laravel projects",
    "keywords": ["php", "laravel", "coding-standard", "phpcs", "php-code-sniffer", "phpcs-standard", "phpcs-ruleset", "phpcs-standard-laravel", "devanox", "mrchetan"],
    "type": "phpcodesniffer-standard",
    "require": {
        "squizlabs/php_codesniffer": "^4.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Chetan",
            "email": "contact@mrchetan.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "5.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
